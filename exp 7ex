#include <stdio.h> 
#include <math.h> 
int main() { 
double population = 100000.0; 
double rate = 0.10; 
int years = 10; 
printf("Name priyanshu\nSAP ID -590027008\nCourse-BCA\nBatch-B5\n");
 printf("Year\tPopulation\n");
 for(int i = 1; i <= years; i++) { 
population = population * (1 + rate); 
printf("%d\t%.0lf\n", i, population); 
} 
return 0; 
} 
